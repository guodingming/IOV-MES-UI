.GooFlow{background:#F5F5F5;border:#ccc 1px solid;-moz-user-select:none;-webkit-user-select:none;border-radius:4px;color:#333}
.GooFlow_head{clear:both;height:20px;padding:1px;border-bottom:#00B4E1 2px solid}
.GooFlow_head label{font-weight:700;display:block;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;height:18px;padding:2px;width:170px;text-align:center;background:#00B4E1;float:left;color:#fff;border-radius:3px 0 0 0;overflow:hidden;margin:-1px 4px 0 -1px}
.GooFlow_head span{float:left;height:22px;width:0;overflow:hidden;border-left:#3892D3 1px solid;margin:0 3px}
.GooFlow_head_btn{display:block;border:0;height:18px;width:18px;cursor:default;padding:1px;margin:0 3px;float:left;outline:0}
.GooFlow_head_btn i{display:inline-block;overflow:hidden;width:18px;height:18px;border:0}
.GooFlow_head_btn:hover{border-radius:2px;background:#FFBF00}
.GooFlow_tool{float:left;padding:0 3px;overflow:hidden;clear:left;border-right:#ccc 1px solid}
.GooFlow_tool_div{overflow:hidden;border-radius:2px;width:24px;padding:2px}
.GooFlow_tool span{height:0;overflow:hidden;border-top:#ccc 1px solid;margin:3px 1px;clear:both;display:block}
.GooFlow_tool_btn{display:block;border:0;height:18px;width:18px;cursor:default;padding:2px;margin:3px 1px;outline:0}
.GooFlow_tool_btn i{display:block;overflow:hidden;width:18px;height:18px;border:0}
.GooFlow_tool_btn:hover{border-radius:3px;padding:0;border:#99B1CE 1px solid;width:20px;height:20px;background:#fff}
.GooFlow_tool_btn:hover i{margin:1px;border-radius:2px}
.GooFlow_tool_btndown{cursor:default;margin:3px 1px;outline:0;border-radius:3px;padding:2px;background:#FFBF00;display:block}
.GooFlow_tool_btndown i{display:block;overflow:hidden;width:18px;height:18px;border-radius:2px}
.GooFlow_work{height:100%;width:100%;float:right;margin:0 3px 3px 0;border:#F5F5F5 1px solid;position:relative}
.GooFlow_work .GooFlow_work_inner{height:100%;width:100%;background-image:url("./img/img/gooflow_bg.png");position:relative;overflow:hidden}
.GooFlow_work .GooFlow_work_group{cursor:default;position:absolute;overflow:hidden;top:0;left:0;z-index:2}
.GooFlow_work text{color:#fff}
.GooFlow_area i{display:block;height:18px;width:18px;top:0;left:0;position:absolute;cursor:pointer}
.GooFlow_work svg{display:block;position:absolute;z-index:4}
.GooFlow_work .draw{color:#f30}
.GooFlow_item{position:absolute;background:#A1DCEB;border:#A1DCEB solid 2px;border-radius:3px;background-color:#C1DCFC;box-shadow:1px 1px 2px rgba(99,99,99,2);padding:5px 7px;z-index:5;cursor:move;}
.GooFlow_item.crosshair{cursor:crosshair}
.GooFlow table{padding:1px;border-radius:2px}
.GooFlow td{vertical-align:middle;text-align:center;padding:0;word-wrap:break-word;word-break:break-all}
.GooFlow .ico{width:18px;}
.GooFlow i{display:block;width:18px;height:18px;overflow:hidden}
.GooFlow .item_primary{padding:8px 15px}
.GooFlow .item_round{border-radius:16px;border:#C1DCFC solid 2px;width:33px;height:33px;overflow:visible}
.GooFlow .item_round table{border:0;padding:2px;width:22px;height:22px}
.GooFlow .item_round .span{display:block;text-align:center;position:absolute;top:33px;left:-25px;width:80px;overflow:visible;text-align:center;padding:0;cursor:default;word-wrap:break-word;word-break:break-all;height:30px}
.GooFlow .item_rectangle{border:#C1DCFC solid 2px;width:33px;height:33px;overflow:visible;transform: rotate(-45deg);padding:3px 5px;}
.GooFlow .item_rectangle table{border:0;padding:2px;width:22px;height:22px}
.GooFlow .item_rectangle .span{display:block;text-align:center;position:absolute;top:33px;left:-25px;width:80px;overflow:visible;text-align:center;padding:0;cursor:default;word-wrap:break-word;word-break:break-all;height:30px}
.GooFlow .rs_ghost{position:absolute;display:none;overflow:hidden;border:#8EA4C1 1px dashed;background:#D9E8FB;opacity:.6;z-index:10;padding:5px 7px}
.GooFlow .item_focus{border:#f30 2px solid}
.GooFlow>textarea{position:absolute;border:#3892D3 1px solid;display:none;font:1em Microsoft Yahei;overflow-y:visible;width:100px;z-index:10001;font-size:12px;vertical-align:middle}
.GooFlow .GooFlow_line_oper{width:70px;height:15px;background-color:#D8E8FC;border:#7DA2CE 1px solid;position:absolute;opacity:.5;z-index:10000}
.GooFlow .GooFlow_line_mp{width:9px;height:9px;opacity:.4;overflow:hidden;position:absolute;z-index:9999;background:#333;cursor:crosshair}
.GooFlow .GooFlow_line_move{opacity:.5;overflow:hidden;position:absolute;z-index:9999;}
.GooFlow .GooFlow_line_oper i{display:inline-block;width:15px;height:15px;margin-left:2px;cursor:pointer}
.GooFlow .b_l1{background:url("./img/img/gooflow_line_oper.png") no-repeat 1px 1px}
.GooFlow .b_l2{background:url("./img/img/gooflow_line_oper.png") no-repeat 1px -14px}
.GooFlow .b_l3{background:url("./img/img/gooflow_line_oper.png") no-repeat 1px -29px}
.GooFlow .b_x{background:url("./img/img/gooflow_line_oper.png") no-repeat 1px -44px;margin-left:10px}
.GooFlow .ico_cursor{background:url("./img/img/gooflow_icon.png") no-repeat 2px -20px}
.GooFlow .ico_direct{background:url("./img/img/gooflow_icon.png") no-repeat -137px -20px}
.GooFlow .ico_start{background:url("./img/img/gooflow_icon.png") no-repeat -19px -20px}
.GooFlow .ico_end{background:url("./img/img/gooflow_icon.png") no-repeat -40px -21px}
.GooFlow .ico_gateway{background:url("./img/icon/base-gateway.png") no-repeat}
.GooFlow .ico_process{background:url("./img/icon/process.png") no-repeat}
.GooFlow .ico_process-lowTemp{background:url("./img/icon/process-lowTemp.png") no-repeat}
.GooFlow .ico_process-highTemp{background:url("./img/icon/process-highTemp.png") no-repeat}
.GooFlow .item_menu{display:none;position:absolute;left:0;top:calc(100% + 2px);width:100%;min-width:110px;z-index:1000;padding:5px 0;font-size:14px;list-style:none;background-color:#fff;background-clip:padding-box;border:1px solid rgba(0,0,0,.15);border-radius:4px;box-shadow:0 6px 12px rgba(0,0,0,.175)}
.GooFlow .item_rectangle .item_menu{left:-100%;top:calc(200% + 2px);transform: rotate(45deg)}
.GooFlow .item_menu ul{margin-bottom:0}
.GooFlow .item_menu li{display:block;padding:3px 20px;clear:both;font-weight:400;line-height:1.42857143;color:#333;white-space:nowrap}
.GooFlow .item_menu li:hover{background-color:#EEE;cursor:pointer}
.GooFlow #tool{position:fixed;left:10px;top:20px;bottom:20px;width:267px;border:1px solid #ebebeb;background-color:#f7f7f7;z-index:999}
.GooFlow #tool.ztree li{margin:4px}
.GooFlow #tool.ztree li.forbid a{cursor:not-allowed;color:#999}
.GooFlow #tool.ztree li.forbid a:hover{text-decoration:none}
.GooFlow #tool.ztree li.forbid span{cursor:not-allowed}
.GooFlow #tool.ztree #toolbar{position:absolute;top:10px;right:5px;cursor:pointer;overflow: auto;}
.GooFlow #tool.ztree #toolbar>div{float:left;height:20px;width:20px;margin-right:5px}
.GooFlow #tool.ztree #toolbar>div.cancel{background:url("./img/img/gooflow_icon.png") no-repeat -58px 1px}
.GooFlow #tool.ztree #toolbar>div.repeat{background:url("./img/img/gooflow_icon.png") no-repeat -78px 1px}
.GooFlow #check .modal-dialog{position:absolute;top:0;bottom:0;left:30px;right:30px;width:auto}
.GooFlow #check .modal-content{height:100%}
.GooFlow #check .modal-body{position:absolute;top:40px;bottom:0;left:0;right:0}
.GooFlow #check .modal-body .CodeMirror,
.GooFlow #check .modal-body .CodeMirror .CodeMirror-scroll{height:100%;width:100%}
.GooFlow #head{position:fixed;left:50%;top:20px;height:40px;width:300px;margin-left:-150px;border:1px solid #ebebeb;background-color:#f7f7f7;z-index:999}
.GooFlow #head ul{height:100%;float: left;}
.GooFlow #head ul li{height:100%;float:left;text-align:center;padding:9px 12px;color:#7c7c7c;cursor:pointer}
.GooFlow #head ul li:hover{color:#000}
.GooFlow #head ul li span{display:inline-block;height:16px;width:16px;position:relative;top:2px;margin-right:2px}
.GooFlow #head ul li.save span{background:url("./img/img/gooflow_save.png") no-repeat}
.GooFlow #head ul li.treeshow span{background:url("./img/img/gooflow_tree.png") no-repeat}
.GooFlow #head ul li.treeshow{display:none}
.GooFlow #head ul li.treehide span{background:url("./img/img/gooflow_tree.png") no-repeat}
.GooFlow #head ul li.check span{background:url("./img/img/gooflow_check.png") no-repeat}
.GooFlow #head ul li.exit span{background:url("./img/img/gooflow_close.png") no-repeat}