
	*{
		box-sizing: border-box;
	}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,button,textarea,p,blockquote,th,td { margin:0; padding:0; }
body {color:#414a59; font-size:14px; font-family: Verdana, Arial, Helvetica, sans-serif;}
label{font:400 14px "microsoft yahei";margin:0}
td,th,caption { font-size:14px; }
h1, h2, h3, h4, h5, h6 { font-weight:normal; font-size:100%; margin:0}
address, caption, cite, code, dfn, em, strong, th, var { font-style:normal; font-weight:normal;}
a { color:#555; text-decoration:none; }
a:hover { text-decoration:none; }
a:focus{outline: none;text-decoration:none;}
img { border:none; }
ol,ul,li { list-style:none; }
input, textarea, select, button { font:14px Verdana,Helvetica,Arial,sans-serif; }
table { border-collapse:collapse;table-layout:fixed;white-space: nowrap;}
td{overflow: hidden;text-overflow: ellipsis;}
/*html,body {height:100%;width:100%;overflow: auto;}*/
#app{
  position:absolute;
	top:0;
	bottom:0;
	left:0;
	right:0;
}
#app>.mCustomScrollBox>.mCSB_container_wrapper>.mCSB_container{
	min-width:1300px;
  	min-height:600px;
	height:100%;
}
.app_container{
	position:relative;
	min-width:1300px;
	min-height:600px;
	height:100%;
	width:100%;
	overflow: hidden;
}
.router-container{
	position: absolute;
	top:80px;
	bottom:0;
	left:0px;
	right:0px;
}
.router-container>div{
	height:100%;
	width:100%;
}


.mes-tree {
	position: absolute;
	top: 0;
	left: 0;
	width: 240px;
	height: 100%;
	padding: 20px;
}

.mes-content.withTree {
	position: absolute;
	top: 0;
	left: 240px;
	right: 0;
	height: 100%;
}

.mes-tool {
	position: absolute;
	top: 0;
	height: 56px;
	left: 0;
	right: 0;
	padding: 19px 20px 0;
}
.mes-tool .el-button{
	float: left;
}
.mes-tool .el-button i{
	margin-left:5px
}
.mes-tool .split{
	float: left;
	display: inline-block;
	height:100%;
	width:1px;
	background: #eee;
	margin:0 25px;
}
.mes-tool .el-input{
	width:250px;
	float:right;
}
.mes-table {
	position: absolute;
	top: 60px;
	left: 0;
	right: 0;
	bottom: 0;
	padding: 11px 20px;
}


/*表单*/

.el-dialog__header{
	padding: 18px 20px;
	border-bottom: 1px solid rgb(210,210,210);
}
.el-dialog__title{
	font-weight: normal;
	letter-spacing: 1px;
}
.el-dialog__body{
	padding: 30px 70px 0;
}
.el-form-item__label{
	padding:11px 30px 11px 0;
	color: rgb(97,102,110);
}
.el-select{
	width: 100%;
}
.mes-upload{
	margin-top:22px;
}
input.el-upload__input[type="file"]{
    display: none;
}

.mes-input{
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	background-color: #fff;
	background-image: none;
	border-radius: 4px;
	border: 1px solid #bfcbd9;
	box-sizing: border-box;
	color: #1f2d3d;
	font-size: inherit;
	height: 36px;
	line-height: 1;
	outline: 0;
	padding: 3px 10px;
	transition: border-color .2s cubic-bezier(.645,.045,.355,1);
	width: 100%;
	display: inline-block;
}
.mes-input::-webkit-input-placeholder {
	color: #97a8be
}
.mes-input::-moz-placeholder {
	color: #97a8be
}
.mes-input:-ms-input-placeholder {
	color: #97a8be
}
.mes-input::placeholder {
	color: #97a8be
}
.mes-input:hover {
	border-color: #8391a5
}
.mes-input:focus {
	outline: 0;
	border-color: #20a0ff
}
.mes-input:invalid{
    border-color:red
}
.mes-submit-button{
	display: inline-block;
	line-height: 1;
	white-space: nowrap;
	cursor: pointer;
	border: 1px solid #20a0ff;
	margin: 0;
	padding: 10px 15px;
	border-radius: 4px;	
	color: #fff;
	background-color: #20a0ff;
	letter-spacing: 5px;
}
.mes-submit-button:hover {
	background: #4db3ff;
	border-color: #4db3ff;
	color: #fff;
}



.submit_button{
	padding-top:30px
}
.submit_button .el-form-item__content{
	text-align: right;
}
.el-cascader{
  width: 100%;
}
/*mask*/
.el-loading-mask.mes{
	background-color: rgba(255,255,255,.15);
}
/*下拉多选*/
.el-select__tags{
	white-space: nowrap;
	overflow:hidden;
}
/*滚动条*/
*::-webkit-scrollbar-track{
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
	background-color: #F5F5F5;
}

*::-webkit-scrollbar{
	width: 10px;
	background-color: #F5F5F5;
}

*::-webkit-scrollbar-thumb{
	background-color: #000000;
	border: 2px solid #555555;
}
